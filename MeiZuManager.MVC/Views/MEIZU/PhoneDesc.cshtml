<script src="~/MEIZU/libs/jquery-3.5.1.js"></script>
<script>
    var a;
    @foreach (var item in Model)
    {
        @:a = @item.ShoppingId;
    }
    //点击切换手机详情图片
    let previewImg = $(".preview-div div img");
    window.addEventListener("load", () => {
        $.ajax({
            url: "/MEIZU/GetAllPhoneDescPic",
            type: "post",
            data: {
                ShoppingId: a
            },
        }).done(res => {
            let SSRM = [], SQDX = [], MHDJS = [], XJH = [], YYL = [];
            let imgList = new Array(SSRM, SQDX, MHDJS, XJH, YYL);
            let a = [];
            let fixTwo = $(".fixTwo > a");
            let fixTwoS = $(".fixTwo");
            for (let i = 0; i < res.data.length; i++) {
                if (i % 4 == 0) {
                    a.push(res.data[i].PicName);
                } else {
                    continue;
                }
            }
            for (let j = 0; j < a.length; j++) {
                for (let k = 0; k < res.data.length; k++) {
                    if (res.data[k].PicName == a[j]) {
                        imgList[j].push("http://localhost:2080/MEIZU/" + res.data[k].SonPicPath);
                    }
                }
            }
            for (let i = 0; i < fixTwo.length; i++) {

                fixTwo[i].innerHTML = a[i];
            }
            for (let i = 0; i < fixTwo.length; i++) {
                fixTwoS[i].addEventListener("click",
                    () => {
                        for (let j = 0; j < imgList.length - 1; j++) {
                            previewImg[j].attributes[0].value = imgList[i][j];
                        }
                    });
            }
            console.info(imgList);
        });
    });

</script>
